        <jdbcConnection
                connectionURL="jdbc:mysql://localhost:3306/xmgl"
                driverClass="com.mysql.jdbc.Driver" password="123456" userId="root"/>

        <!-- 生成实体类 -->
        <javaModelGenerator
                targetPackage="com.gedi.projectmanagement.model"
                targetProject="src/main/java"/>

        <!-- mapper.xml -->
        <sqlMapGenerator targetPackage="mapper"
                         targetProject="src/main/resources"/>

        <!-- dao接口 -->
        <javaClientGenerator
                targetPackage="com.gedi.projectmanagement.dao"
                targetProject="src/main/java" type="XMLMAPPER"/>

